  "_moduleAliases": {
    "@": "dist",
    "@configs": "dist/configs"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/express": "4.17.14",
    "commitizen": "4.2.5",
    "cz-git": "1.3.11",
