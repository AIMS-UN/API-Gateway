  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIMS-UN/api-gateway.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/AIMS-UN/api-gateway/issues"
  },
  "homepage": "https://github.com/AIMS-UN/api-gateway#readme",
